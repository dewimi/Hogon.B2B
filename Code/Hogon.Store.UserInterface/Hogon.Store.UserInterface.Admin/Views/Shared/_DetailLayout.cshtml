<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/hogon/common")
    @Styles.Render("~/Content/css/form")
    <link href="~/Content/sweetalert.css" rel="stylesheet" />
    <script src="~/Scripts/sweetalert.min.js"></script>
    <link href="~/Content/bootstrap-treeview.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap-treeview.js"></script>

</head>

<body>
    <div style="width:100%;height:100%">
        @RenderSection("formHeader", false)

        <div class="basicparameter" ng-app="myApp" ng-controller="validateCtrl">
            <form name="myForm" novalidate>
                @RenderBody()
            </form>
       
        </div>
    </div>
</body>
</html>
